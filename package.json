{
  "name": "retailoptimizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && npm run preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run tests/integration/**/*.test.{ts,tsx}",
    "test:component": "vitest run src/components/**/*.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:a11y": "playwright test --project=a11y-chrome",
    "test:accessibility": "npm run test:a11y",
    "test:performance": "playwright test --project=performance",
    "test:lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./test-results/lighthouse.html",
    "test:security": "npm audit && npm run test:security:custom",
    "test:security:custom": "echo 'Running custom security tests...' && vitest run tests/security/**/*.test.{ts,tsx}",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run lint && npm run test:coverage && npm run test:e2e",
    "test:full": "npm run test:all && npm run test:a11y && npm run test:performance",
    "test:setup": "playwright install",
    "test:clean": "rm -rf coverage test-results",
    "test:report": "echo 'Opening test reports...' && open coverage/index.html && open test-results/index.html"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-three/drei": "^9.96.1",
    "@react-three/fiber": "^8.15.19",
    "@stripe/stripe-js": "^7.8.0",
    "@tabler/icons-react": "^3.26.0",
    "@types/three": "^0.174.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.540.0",
    "mini-svg-data-uri": "^1.4.4",
    "motion": "^11.15.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-use": "^17.5.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.174.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.0",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.47.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.51.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^2.0.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.0",
    "c8": "^10.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^15.0.0",
    "jsdom": "^24.0.0",
    "lighthouse": "^12.0.0",
    "msw": "^2.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^2.0.0"
  }
}
